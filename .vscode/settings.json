{
  "deno.enable": false,
  "deno.lint": false,
  "deno.unstable": false,
  "cmake.configureOnOpen": true,
  "cmake.sourceDirectory": "${workspaceFolder}/netvr-cpp",
  "cmake.buildDirectory": "${workspaceFolder}/netvr-cpp/build",
  "typescript.tsdk": "./netvr-server/node_modules/typescript/lib",
  "cmake.buildArgs": ["--config Release"],
  "grammarly.audience": "knowledgeable",
  "grammarly.emotions": ["neutral"],
  "grammarly.hideUnavailablePremiumAlerts": true,
  "grammarly.domain": "academic",
  "grammarly.dialect": "british",
  "grammarly.goals": ["inform", "describe"],
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80
  },
  "zenMode.hideLineNumbers": false,
  "ltex.language": "en-GB",
  "ltex.disabledRules": {
    "en-GB": ["UNLIKELY_OPENING_PUNCTUATION"]
  },
  "ltex.dictionary": {
    "en-GB": [":./dictionary.txt"]
  }
}
